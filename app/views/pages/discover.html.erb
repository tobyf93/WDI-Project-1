<div id="discover" class="row">

  <!-- First Half -->
  <div class="col-md-6">
    <div class="container-fluid">

      <!-- File List -->
      <div class="row">
        <div id="fileList" class="list-group">
          <% @items.reverse_each do |item|%>
            <a href="#" class="list-group-item" data-cl="<%= item.cl_id %>"><%= item.name %></a>
          <% end %>
        </div>
      </div>

      <!-- Google Map -->
      <div class="row">
        <div class="col-md-12">
          <div id="mapCanvas"></div>
        </div>
      </div>

    </div>
  </div>

  <!-- Second Half - File Preview-->
  <div class="col-md-6">
    <div id="filePreview"class="container-fluid well">
      <%= image_tag('usb.png') %>
    </div>
  </div>
</div>

